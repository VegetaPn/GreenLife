<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
  <servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>com.greenlife.client.controller.TestServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/TestServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ReAddressServlet</servlet-name>
    <servlet-class>com.greenlife.client.controller.ReAddressServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ReAddressServlet</servlet-name>
    <url-pattern>/reAddress</url-pattern>
  </servlet-mapping>
</web-app>
